{
  "name": "ririko-ai",
  "version": "0.3.0",
  "description": "A powerful AI-powered general Discord bot that you can call your companion",
  "author": "Earnest Angel",
  "email": "me@angel.net.my",
  "url": "https://angel.net.my/",
  "funding": "https://angel.net.my/donate",
  "repository": {
    "type": "git",
    "url": "https://github.com/RirikoAI/RirikoBot.git"
  },
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "AI",
    "Chatbot",
    "General Discord bot",
    "gpt-neox-20b"
  ],
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "export NODE_ENV=production || set NODE_ENV=production && npm run index",
    "start:nowatch": "export NODE_ENV=development || set NODE_ENV=development && npm run build-and-start",
    "start:dev": "export NODE_ENV=development || set NODE_ENV=development && nodemon --exec npm run build-and-start",
    "build-and-start": "npm-run-all -s build index",
    "build": "babel src --out-dir dist --extensions \".ts,.js\" --copy-files --delete-dir-on-start",
    "index": "node index.js",
    "test": "jest --coverage --verbose",
    "test:silent": "jest --coverage --silent",
    "heroku-prebuild": "npm install --dev && cp -n config.example.js config.js"
  },
  "engines": {
    "node": "16.15.1"
  },
  "dependencies": {
    "@discordjs/builders": "^1.4.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.5.0",
    "@discordjs/voice": "^0.14.0",
    "@distube/deezer": "^1.0.0",
    "@distube/soundcloud": "^1.3.0",
    "@distube/spotify": "^1.5.1",
    "@distube/yt-dlp": "^1.1.3",
    "@distube/ytdl-core": "^4.11.7",
    "@vitalets/google-translate-api": "^9.1.0",
    "babel-preset-es2015": "^6.24.1",
    "better-sqlite3": "^8.1.0",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "decimal.js": "^10.4.3",
    "discord-api-types": "^0.37.35",
    "discord.js": "^14.5.0",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.1.0",
    "genius-lyrics": "^4.4.3",
    "iso-639-1": "^2.1.15",
    "jest": "^29.4.3",
    "libsodium-wrappers": "^0.7.11",
    "mathjs": "^11.6.0",
    "moment": "^2.29.4",
    "mongodb": "^5.1.0",
    "mongoose": "^6.4.4",
    "ms": "^2.1.3",
    "nekos.life": "^3.0.0",
    "nlpcloud": "^1.0.41",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "openai": "^3.2.1",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "pretty-ms": "^8.0.0",
    "quick.db": "^9.0.5",
    "random-puppy": "^1.1.0",
    "sapphire": "^0.0.3",
    "save-dev": "^0.0.1-security",
    "songlyrics": "^2.4.5",
    "source-map-support": "^0.5.21",
    "sourcebin_js": "^0.0.3-ignore"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-source-map-support": "^2.2.0",
    "babel-preset-minify": "^0.5.2",
    "nodemon": "^2.0.21"
  }
}
